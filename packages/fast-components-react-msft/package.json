{
  "name": "@microsoft/fast-components-react-msft",
  "description": "FAST Microsoft components implemented in React",
  "sideEffects": false,
  "version": "4.29.1",
  "author": {
    "name": "Microsoft",
    "url": "https://discord.gg/FcSNfg4"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Microsoft/fast-dna.git"
  },
  "bugs": {
    "url": "https://github.com/Microsoft/fast-dna/issues/new/choose"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p ./tsconfig.build.json && yarn build:api && yarn copy:all && yarn copy:api",
    "build:api": "yarn clean:api && yarn build:theme && typedoc --tsconfig ./tsconfig.build.json && yarn convert:api",
    "build:theme": "tsc -p ./tsconfig.api-theme.json",
    "clean:api": "node ../../build/clean.js api",
    "clean:dist": "node ../../build/clean.js dist",
    "convert:readme": "node ../../build/convert-readme.js .tmp",
    "copy:all": "yarn convert:json-schema && yarn copy:readme",
    "convert:json-schema": "node ../../build/convert-schemas.js",
    "convert:api": "node ./build/create-component-api-documents.js --ignore-dir utilities --component-dir ./src --typedoc-src ./api",
    "copy:api": "node ../../build/copy.js --src './src/**/API.md' --dist 'dist'",
    "copy:readme": "node ../../build/copy.js --src './src/**/README.md' --dist 'dist'",
    "prepare": "yarn clean:dist && yarn build",
    "prettier": "prettier --config ../../.prettierrc --write \"**/*.{ts,tsx}\"",
    "prettier:diff": "prettier --config ../../.prettierrc \"**/*.{ts,tsx}\" --list-different",
    "start": "start-storybook",
    "test": "yarn convert:readme && yarn tslint && yarn unit-tests",
    "tslint": "tslint -c ./tslint.json '**/*.ts{,x}'",
    "tslint:fix": "tslint -c ./tslint.json --fix '**/*.ts{,x}'",
    "unit-tests": "jest --runInBand",
    "unit-tests:watch": "jest --watch",
    "watch": "tsc -p ./tsconfig.build.json -w --preserveWatchOutput"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./mock.js"
    ],
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "statements": 95,
        "branches": 84,
        "functions": 95,
        "lines": 95
      }
    },
    "coveragePathIgnorePatterns": [
      "/.tmp/*"
    ],
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "!<rootDir>/node_modules/lodash-es",
      "node_modules/css-vendor"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@microsoft/fast-colors": "^5.0.2",
    "@microsoft/fast-jss-manager-react": "^4.6.1",
    "@microsoft/fast-storybook-presets": "^1.1.31",
    "@microsoft/fast-tooling-react": "^1.31.0",
    "@microsoft/fast-tslint-rules": "^3.0.4",
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/preset-typescript": "^1.0.0",
    "@storybook/react": "^5.1.1",
    "@types/jest": "^24.0.11",
    "@types/lodash-es": "^4.17.0",
    "@types/node": "^9.4.6",
    "@types/react": "^16.8.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.6",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eyes.images": "3.6.2",
    "file-loader": "^1.1.11",
    "focus-visible": "^4.1.5",
    "glob": "^7.1.6",
    "jest": "^24.5.0",
    "linkify-it": "^2.2.0",
    "lodash-es": "^4.0.0",
    "mkdirp": "^0.5.1",
    "prettier": "1.14.3",
    "raf-throttle": "^2.0.3",
    "react": "^16.8.0",
    "react-docgen-typescript-loader": "^3.1.0",
    "react-dom": "^16.8.0",
    "rimraf": "^2.6.2",
    "storybook-readme": "^5.0.8",
    "ts-jest": "^24.0.0",
    "ts-loader": "^4.0.1",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.15.0",
    "tslint-loader": "^3.6.0",
    "typedoc": "^0.15.4",
    "typedoc-plugin-markdown": "^2.2.14",
    "typescript": "3.5.1",
    "yargs": "^11.0.0"
  },
  "peerDependencies": {
    "@microsoft/fast-jss-manager-react": "^3.1.13 || ^4.0.0",
    "lodash-es": "^4.0.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "dependencies": {
    "@microsoft/fast-components-class-name-contracts-base": "^4.6.0",
    "@microsoft/fast-components-class-name-contracts-msft": "^4.8.0",
    "@microsoft/fast-components-foundation-react": "^3.1.3",
    "@microsoft/fast-components-react-base": "^4.23.6",
    "@microsoft/fast-components-styles-msft": "^4.28.0",
    "@microsoft/fast-jss-utilities": "^4.7.5",
    "@microsoft/fast-web-utilities": "^4.4.0",
    "exenv-es6": "^1.0.0",
    "global": "^4.4.0",
    "transform-markdown-links": "^2.0.0",
    "typedoc": "^0.15.4",
    "utility-types": "^2.1.0"
  }
}
