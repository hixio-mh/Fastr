{
  "name": "@microsoft/fast-components-react-msft",
  "description": "FAST Microsoft components implemented in React",
  "sideEffects": false,
  "version": "4.30.6",
  "author": {
    "name": "Microsoft",
    "url": "https://discord.gg/FcSNfg4"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Microsoft/fast-dna.git"
  },
  "bugs": {
    "url": "https://github.com/Microsoft/fast-dna/issues/new/choose"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p ./tsconfig.build.json && yarn copy:all",
    "clean:dist": "node ../../build/clean.js dist",
    "convert:readme": "node ../../build/convert-readme.js .tmp",
    "copy:all": "yarn convert:json-schema && yarn copy:readme",
    "convert:json-schema": "node ../../build/convert-schemas.js",
    "copy:readme": "node ../../build/copy-readme.js",
    "prepare": "yarn clean:dist && yarn build",
    "prettier": "prettier --config ../../.prettierrc --write \"**/*.{ts,tsx}\"",
    "prettier:diff": "prettier --config ../../.prettierrc \"**/*.{ts,tsx}\" --list-different",
    "start": "start-storybook",
    "test": "yarn convert:readme && yarn tslint && yarn unit-tests",
    "tslint": "tslint -c ./tslint.json '**/*.ts{,x}'",
    "tslint:fix": "tslint -c ./tslint.json --fix '**/*.ts{,x}'",
    "unit-tests": "jest --runInBand",
    "unit-tests:watch": "jest --watch",
    "watch": "tsc -p ./tsconfig.build.json -w --preserveWatchOutput"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./mock.js"
    ],
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "statements": 95,
        "branches": 84,
        "functions": 95,
        "lines": 95
      }
    },
    "coveragePathIgnorePatterns": [
      "/.tmp/*"
    ],
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "!<rootDir>/node_modules/lodash-es",
      "node_modules/css-vendor"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@microsoft/fast-colors": "^5.0.4",
    "@microsoft/fast-jss-manager-react": "^4.6.3",
    "@microsoft/fast-storybook-presets": "^1.1.36",
    "@microsoft/fast-tooling-react": "^1.32.3",
    "@microsoft/fast-tslint-rules": "^3.1.0",
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/preset-typescript": "^1.0.0",
    "@storybook/react": "^5.1.1",
    "@types/jest": "^25.2.1",
    "@types/lodash-es": "^4.17.0",
    "@types/node": "^9.4.6",
    "@types/react": "^16.8.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^25.2.6",
    "babel-loader": "^8.0.6",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eyes.images": "3.6.2",
    "file-loader": "^1.1.11",
    "focus-visible": "^4.1.5",
    "jest": "^25.2.7",
    "lodash-es": "^4.0.0",
    "prettier": "1.14.3",
    "raf-throttle": "^2.0.3",
    "react": "^16.8.0",
    "react-docgen-typescript-loader": "^3.1.0",
    "react-dom": "^16.8.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^25.3.1",
    "ts-loader": "^4.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-loader": "^3.6.0",
    "typescript": "3.3.3",
    "yargs": "^11.0.0"
  },
  "peerDependencies": {
    "@microsoft/fast-jss-manager-react": "^3.1.13 || ^4.0.0",
    "lodash-es": "^4.0.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "dependencies": {
    "@microsoft/fast-components-class-name-contracts-base": "^4.6.2",
    "@microsoft/fast-components-class-name-contracts-msft": "^4.8.2",
    "@microsoft/fast-components-foundation-react": "^3.1.4",
    "@microsoft/fast-components-react-base": "^4.25.6",
    "@microsoft/fast-components-styles-msft": "^4.28.5",
    "@microsoft/fast-jss-utilities": "^4.7.8",
    "@microsoft/fast-web-utilities": "^4.4.1",
    "exenv-es6": "^1.0.0",
    "utility-types": "^2.1.0"
  }
}
