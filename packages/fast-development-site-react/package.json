{
  "name": "@microsoft/fast-development-site-react",
  "description": "FAST development testing site for React",
  "sideEffects": false,
  "version": "4.4.23",
  "author": {
    "name": "Microsoft",
    "url": "https://discord.gg/FcSNfg4"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Microsoft/fast-dna.git"
  },
  "bugs": {
    "url": "https://github.com/Microsoft/fast-dna/issues/new/choose"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p ./tsconfig.build.json",
    "build:app": "webpack --progress --mode=production",
    "clean:dist": "node ../../build/clean.js dist",
    "convert:readme": "node ../../build/convert-readme.js --src=app/**/README.md .tmp",
    "prepare": "npm run clean:dist && npm run build",
    "prettier": "prettier --config ../../.prettierrc --write \"**/*.{ts,tsx}\"",
    "prettier:diff": "prettier --config ../../.prettierrc \"**/*.{ts,tsx}\" --list-different",
    "start": "webpack-dev-server --progress",
    "test": "npm run convert:readme && npm run tslint && npm run build:app",
    "tslint": "tslint -c ./tslint.json '**/*.ts{,x}'",
    "tslint:fix": "tslint -c ./tslint.json --fix '**/*.ts{,x}'",
    "watch": "npm run build -- -w --preserveWatchOutput"
  },
  "dependencies": {
    "@microsoft/fast-glyphs-msft": "^4.0.2",
    "@microsoft/fast-jss-utilities": "^4.6.6",
    "@microsoft/fast-layouts-react": "^4.5.9",
    "@microsoft/fast-tooling-react": "^1.20.0",
    "@microsoft/fast-web-utilities": "^4.3.2",
    "react-syntax-highlighter": "^7.0.4"
  },
  "devDependencies": {
    "@microsoft/fast-components-class-name-contracts-base": "^4.3.3",
    "@microsoft/fast-jss-manager": "^4.1.7",
    "@microsoft/fast-jss-manager-react": "^4.5.0",
    "@microsoft/fast-tslint-rules": "^3.0.3",
    "@polymer/polymer": "^3.0.0-pre.10",
    "@types/lodash-es": "^4.17.0",
    "@types/node": "^9.6.7",
    "@types/react": "^16.4.18",
    "@types/react-router": "^4.0.0",
    "fork-ts-checker-webpack-plugin": "^0.4.1",
    "html-webpack-plugin": "^3.0.4",
    "lodash-es": "^4.0.0",
    "prettier": "1.14.3",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-router-dom": "^4.2.2",
    "rimraf": "^2.6.2",
    "ts-loader": "^4.0.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.15.0",
    "tslint-loader": "^3.6.0",
    "tslint-react": "^3.5.1",
    "typescript": "3.3.3",
    "webpack": "^4.1.0",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.0",
    "webpack-shell-plugin": "^0.5.0",
    "yargs": "^11.0.0"
  },
  "peerDependencies": {
    "@microsoft/fast-jss-manager-react": "^3.1.13 || ^4.0.0",
    "lodash-es": "^4.0.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-router-dom": "^4.2.2"
  }
}
