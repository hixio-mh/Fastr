{
  "name": "@microsoft/fast-sketch-library",
  "description": "A Node.js utility for quickly extracting Sketch symbol libraries from a website",
  "version": "4.0.5",
  "private": true,
  "sideEffects": false,
  "author": {
    "name": "Microsoft",
    "url": "https://discord.gg/FcSNfg4"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Microsoft/fast-dna.git"
  },
  "bugs": {
    "url": "https://github.com/Microsoft/fast-dna/issues/new/choose"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean:dist": "node ../../build/clean.js dist",
    "prepare": "yarn clean:dist && tsc -p ./tsconfig.json && webpack",
    "prettier": "prettier --config ../../.prettierrc --write \"**/*.{ts,tsx}\"",
    "prettier:diff": "prettier --config ../../.prettierrc \"**/*.{ts,tsx}\" --list-different",
    "test": "yarn eslint",
    "eslint": "eslint . --ext .ts,.tsx",
    "eslint:fix": "eslint . --ext .ts,.tsx --fix"
  },
  "devDependencies": {
    "@brainly/html-sketchapp": "^3.1.0",
    "@microsoft/eslint-config-fast-dna": "^1.1.0",
    "@types/node": "^9.4.6",
    "@types/puppeteer": "^1.3.2",
    "eslint-config-prettier": "^6.10.1",
    "prettier": "2.0.2",
    "ts-loader": "^4.3.0",
    "typescript": "3.3.3",
    "webpack": "^4.10.2",
    "webpack-cli": "^3.1.1"
  },
  "dependencies": {
    "puppeteer": "^1.4.0"
  }
}
