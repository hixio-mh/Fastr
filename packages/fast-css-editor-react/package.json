{
  "name": "@microsoft/fast-css-editor-react",
  "version": "4.4.0",
  "description": "A set of React components that allows the user to edit CSS properties",
  "sideEffects": false,
  "author": {
    "name": "Microsoft",
    "email": "fwq@microsoft.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Microsoft/fast-dna.git"
  },
  "bugs": {
    "url": "https://github.com/Microsoft/fast-dna/issues/new/choose"
  },
  "readme": "README.md",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p ./tsconfig.json && npm run copy:all",
    "build:app": "webpack --progress --mode=production",
    "clean:dist": "node ../../build/clean.js dist",
    "copy:all": "npm run copy:json-schema",
    "copy:json-schema": "node ../../build/copy-schemas.js",
    "prepare": "npm run clean:dist && npm run build",
    "prettier": "prettier --config ../../.prettierrc --write \"**/*.{ts,tsx}\"",
    "prettier:diff": "prettier --config ../../.prettierrc \"**/*.{ts,tsx}\" --list-different",
    "start": "webpack-dev-server",
    "test": "npm run tslint && npm run unit-tests && npm run build:app",
    "tslint": "tslint -c ./tslint.json '**/*.ts{,x}'",
    "tslint:fix": "tslint -c ./tslint.json --fix '**/*.ts{,x}'",
    "unit-tests": "jest --maxWorkers=2",
    "unit-tests:watch": "jest --watch",
    "watch": "tsc -p ./tsconfig.json -w --preserveWatchOutput"
  },
  "jest": {
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "statements": 80,
        "branches": 80,
        "functions": 80,
        "lines": 80
      }
    },
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "!<rootDir>/node_modules/lodash-es",
      "node_modules/jss-*",
      "node_modules/css-vendor"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@microsoft/fast-application-utilities": "^3.1.3",
    "@microsoft/fast-components-class-name-contracts-base": "^3.10.0",
    "@microsoft/fast-glyphs-msft": "^3.1.1",
    "@microsoft/fast-jss-manager": "^3.0.13",
    "@microsoft/fast-jss-manager-react": "^3.1.14",
    "@microsoft/fast-jss-utilities": "^3.2.5",
    "@microsoft/fast-sketch-library": "^3.0.5",
    "@microsoft/fast-tslint-rules": "^3.0.2",
    "@types/jest": "^24.0.11",
    "@types/lodash-es": "^4.17.0",
    "@types/node": "^9.4.6",
    "@types/react": "^16.4.18",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.5.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "html-webpack-plugin": "^3.0.4",
    "jest": "^24.5.0",
    "lodash-es": "^4.0.0",
    "prettier": "1.14.3",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-test-renderer": "^16.6.3",
    "ts-jest": "^24.0.0",
    "ts-loader": "^4.0.1",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.15.0",
    "tslint-loader": "^3.6.0",
    "typescript": "^3.0.1",
    "webpack": "^4.1.0",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.0"
  },
  "peerDependencies": {
    "@microsoft/fast-application-utilities": "^3.0.0",
    "@microsoft/fast-components-class-name-contracts-base": "^3.0.0",
    "@microsoft/fast-jss-manager-react": "^3.0.0",
    "@microsoft/fast-jss-utilities": "^3.0.0",
    "lodash-es": "^4.0.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0"
  },
  "dependencies": {
    "@microsoft/fast-components-foundation-react": "^2.1.4",
    "utility-types": "^2.1.0"
  }
}
