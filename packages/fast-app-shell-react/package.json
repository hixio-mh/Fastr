{
  "name": "@microsoft/fast-app-shell-react",
  "description": "FAST Microsoft application shell",
  "sideEffects": false,
  "version": "0.0.0",
  "author": {
    "name": "Microsoft",
    "url": "https://discord.gg/FcSNfg4"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Microsoft/fast-dna.git"
  },
  "bugs": {
    "url": "https://github.com/Microsoft/fast-dna/issues/new/choose"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p ./tsconfig.build.json",
    "build:app": "webpack --progress --mode=production",
    "clean:dist": "node ../../build/clean.js dist",
    "prepare": "npm run clean:dist && npm run build",
    "prettier": "prettier --config ../../.prettierrc --write \"**/*.{ts,tsx}\"",
    "prettier:diff": "prettier --config ../../.prettierrc \"**/*.{ts,tsx}\" --list-different",
    "start": "webpack-dev-server",
    "test": "npm run tslint && npm run unit-tests && npm run build:app",
    "tslint": "tslint -c ./tslint.json '**/*.ts{,x}'",
    "tslint:fix": "tslint -c ./tslint.json --fix '**/*.ts{,x}'",
    "unit-tests": "jest --maxWorkers=2",
    "unit-tests:watch": "jest --watch",
    "watch": "tsc -p ./tsconfig.build.json -w --preserveWatchOutput"
  },
  "jest": {
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "statements": 95,
        "branches": 70,
        "functions": 95,
        "lines": 95
      }
    },
    "coveragePathIgnorePatterns": [
      "/.tmp/*"
    ],
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "!<rootDir>/node_modules/lodash-es",
      "node_modules/jss-*",
      "node_modules/css-vendor"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@microsoft/fast-jss-manager": "^4.0.1",
    "@microsoft/fast-jss-manager-react": "^4.2.1",
    "@microsoft/fast-tslint-rules": "^3.0.3",
    "@types/jest": "^24.0.11",
    "@types/lodash-es": "^4.17.0",
    "@types/node": "^9.4.6",
    "@types/react": "^16.4.18",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.5.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "focus-visible": "^4.1.5",
    "html-webpack-plugin": "^3.0.4",
    "jest": "^24.5.0",
    "lodash-es": "^4.0.0",
    "prettier": "1.14.3",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-router-dom": "^5.0.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^24.0.0",
    "ts-loader": "^4.0.1",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.15.0",
    "tslint-loader": "^3.6.0",
    "typescript": "3.3.3",
    "webpack": "^4.1.0",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.0"
  },
  "peerDependencies": {
    "@microsoft/fast-jss-manager-react": "^3.1.13",
    "lodash-es": "^4.0.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-router-dom": "^5.0.0"
  },
  "dependencies": {
    "@microsoft/fast-components-class-name-contracts-base": "^4.0.1",
    "@microsoft/fast-components-class-name-contracts-msft": "^4.1.0",
    "@microsoft/fast-components-foundation-react": "^3.0.1",
    "@microsoft/fast-components-react-base": "^4.1.5",
    "@microsoft/fast-components-react-msft": "^4.3.0",
    "@microsoft/fast-components-styles-msft": "^4.4.0",
    "@microsoft/fast-glyphs-msft": "^4.0.1",
    "@microsoft/fast-jss-utilities": "^4.2.1",
    "@microsoft/fast-layouts-react": "^4.1.4",
    "@microsoft/fast-web-utilities": "^4.0.1",
    "exenv-es6": "^1.0.0",
    "raf-throttle": "^2.0.3",
    "utility-types": "^2.1.0"
  }
}
