    "prettier:diff": "prettier --config ../../../.prettierrc \"**/*.ts\" --list-different",
    "test": "yarn eslint",
    "eslint": "eslint . --ext .ts",
    "eslint:fix": "eslint . --ext .ts --fix",
    "watch": "yarn build -- -w --preserveWatchOutput"
  },
  "devDependencies": {
    "@microsoft/eslint-config-fast-dna": "^1.1.1",
